{
  "name": "movie-collection",
  "version": "1.0.0",
  "author": "Nicolas Jardim <nicolasemkis@gmail.com>",
  "scripts": {
    "postinstall": "npm-run-all clean-modules",
    "prepare": "husky install",
    "start": "vite --port 8080",
    "build": "npm-run-all type-checking vite:build optimize-css",
    "type-checking": "vue-tsc --noEmit",
    "vite:build": "vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{vue,ts,js}\" --fix",
    "format": "prettier --write \"src/**/*.{json,css,scss}\"",
    "clean-modules": "modclean --run",
    "optimize-css": "postcss dist/**/*.css --replace --verbose"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "core-js": "^3.19.3",
    "hash-string": "^1.0.0",
    "lodash-es": "^4.17.21",
    "modern-css-reset": "^1.4.0",
    "modern-normalize": "^1.1.0",
    "msw": "^0.36.3",
    "sass-rem": "^3.0.0",
    "vue": "^3.2.26",
    "vue-query": "^1.14.0",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.5",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "eslint": "^8.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "modclean": "^3.0.0-beta.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-combine-media-query": "^1.0.1",
    "prettier": "^2.5.1",
    "sass": "^1.45.0",
    "typescript": "^4.5.3",
    "vite": "^2.7.1",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.29.8"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
